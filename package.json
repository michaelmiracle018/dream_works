{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "turbo format lint typecheck coverage build knip",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "knip": "knip",
    "lint": "eslint --color --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "lint --fix",
    "prepare": "is-ci || lefthook install",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.0"
  },
  "dependencies": {
    "@tanstack/react-query": "5.90.21",
    "@tanstack/react-router": "1.159.5",
    "framer-motion": "^12.34.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-icons": "^5.5.0",
    "react-odometerjs": "^3.1.3",
    "swiper": "^12.1.0"
  },
  "devDependencies": {
    "@iconify-json/logos": "1.2.10",
    "@iconify-json/lucide": "1.2.90",
    "@jimmy.codes/eslint-config": "7.3.0",
    "@jimmy.codes/prettier-config": "2.1.2",
    "@playwright/test": "1.58.2",
    "@tanstack/react-query-devtools": "5.91.3",
    "@tanstack/react-router-devtools": "1.160.2",
    "@tanstack/router-vite-plugin": "1.160.2",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/bun": "1.3.9",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "eslint": "^10.0.0",
    "gitzy": "6.5.7",
    "happy-dom": "20.6.1",
    "is-ci": "4.1.0",
    "jiti": "2.6.1",
    "knip": "5.83.1",
    "lefthook": "2.1.1",
    "prettier": "^3.8.1",
    "turbo": "2.8.9",
    "typescript": "5.9.3",
    "vite": "npm:rolldown-vite@7.1.0",
    "vite-tsconfig-paths": "6.1.1",
    "vitest": "4.0.18"
  },
  "packageManager": "yarn@1.22.22",
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "oxc-resolver",
    "unrs-resolver"
  ]
